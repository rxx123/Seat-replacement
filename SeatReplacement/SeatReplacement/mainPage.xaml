<Page x:Class="SeatReplacement.mainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SeatReplacement"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="mainPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="30" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Image x:Name="imgIdentity" Grid.Column="0" Grid.Row="0" Source="image/identityS.png" Stretch="None" VerticalAlignment="Bottom" />
        <Image x:Name="imgTicket" Grid.Column="0" Grid.Row="2" Source="image/ticketS.png" Stretch="None" VerticalAlignment="Top"  />
    </Grid>
</Page>
